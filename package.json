{
  "name": "metal-layout",
  "version": "0.0.0",
  "description": "A Metal.js component that can render bootstrap grid layouts",
  "license": "BSD",
  "repository": "metal/metal-layout",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "lib/all/layout.js",
  "jsnext:main": "src/all/layout.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "gulp build",
    "compile": "babel --presets metal -d lib/ src/",
    "prepublish": "gulp soy && npm run compile",
    "test": "gulp test",
    "watch": "gulp build:watch"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal-drag-drop": "^1.0.0",
    "metal-soy": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-metal": "^4.0.0",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0"
  }
}
